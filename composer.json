{
    "name": "dtl/bolt-fixtures",
    "description": "Fixtures for Bolt",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": "^3.0"
    },
    "authors": [
        {
            "name": "Daniel Leech",
            "email": "daniel@dantleech.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "DTL\\Bolt\\Extension\\Fixtures\\": "src/"
        }
    },
    "require-dev": {
        "nelmio/alice": "^2.1",
        "phpunit/phpunit": "^5.0",
        "passwordlib/passwordlib": "^1.0@beta"
    },
    "suggest": {
        "nelmio/alice": "In order that this extension works."
    },
    "autoload-dev": {
        "psr-4": {
            "DTL\\Bolt\\Extension\\Fixtures\\Tests\\": "tests/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "DTL\\Bolt\\Extension\\Fixtures\\DtlBoltFixturesExtension"
    }
}
